{
    "version": 2,
    "builds": [
        {
        "src": "mlm/wsgi.py",
        "use": "@vercel/python",
        "config":{ "maxLambdaSize": "15mb", "runtime": "3.9"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/staticfiles",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/"
        },
        {
            "src": "/(.*)",
            "dest": "mlm.wsgi.py"
        }
    ]
}